<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scampa.starbuzzapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CatalogueActivity"
            android:label="@string/title_activity_catalogue" >
        </activity>
        <activity
            android:name=".CatalogueItemActivity"
            android:label="@string/title_activity_catalogue_item" >
        </activity>
        <activity
            android:name=".ClientsPortfolioActivity"
            android:label="@string/title_activity_clients_portfolio" >
        </activity>
        <activity
            android:name=".ClientsActivity"
            android:label="@string/title_activity_clients" >
        </activity>
    </application>

</manifest>
